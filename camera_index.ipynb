    "import cv2\n",
    "\n",
    "for i in range (-5,10):\n",
    "    try:\n",
    "        cap = cv2.VideoCapture(i)\n",
    "        ret ,frame = cap.read()\n",
    "        cv2.imwrite(f'{i}.jpg',frame)\n",
    "        print(i)\n",
    "        cap.release()\n",
    "    except:\n",
    "        pass"
   ]
